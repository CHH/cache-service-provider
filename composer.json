{
    "name": "chh/cache-service-provider",
    "description": "A Cache Service Provider for Silex, using Doctrine\\Common\\Cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Christoph Hochstrasser",
            "email": "christoph.hochstrasser@gmail.com"
        }
    ],
    "keywords": [
        "silex", "cache"
    ],
    "require": {
        "php": ">=5.3.3",
        "silex/silex": "~1.0",
        "doctrine/cache": "~1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "chh/bob": "1.0.*@dev",
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-0": {
            "CHH\\Silex\\": "lib\/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
